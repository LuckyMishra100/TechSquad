@model IEnumerable<TechSquad.Models.Models.ApplicationUser>
@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
    <table  border="1">
        <thead>
            <tr>
                <th>User Name</th>
                <th>Email</th>
                <th>Phone No.</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@item.PhoneNumber</td>
                <td><a href="~/Admin/AddUser" >Add </a>| 
                    <a href="~/Admin/AddUser">Update </a>|
                    <a href="~/Admin/DeleteUser"> Delete</a>
</td>
            </tr>
                }
            }
            
        </tbody>
    </table>
</div>